{
  "name": "@tx/agent-sdk",
  "version": "0.5.0",
  "description": "TypeScript SDK for building custom agents with TX task management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "bunx --bun vitest run --passWithNoTests"
  },
  "dependencies": {
    "@jamesaphoenix/tx-types": "*"
  },
  "optionalDependencies": {
    "@jamesaphoenix/tx-core": "*",
    "effect": "^3.19.15"
  },
  "devDependencies": {
    "@jamesaphoenix/tx-core": "*",
    "@types/bun": "^1.2.0",
    "@types/node": "^25.2.0",
    "effect": "^3.19.15",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "tx",
    "agent",
    "sdk",
    "task-management",
    "ai-agents"
  ]
}
