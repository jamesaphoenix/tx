{
  "name": "@jamesaphoenix/tx",
  "version": "0.5.3",
  "description": "Headless, local infra for AI agents - primitives for memory, tasks, and orchestration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "bun": "./src/types.ts",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jamesaphoenix/tx-core": "*",
    "@jamesaphoenix/tx-types": "*"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesaphoenix/tx.git",
    "directory": "packages/tx"
  },
  "keywords": [
    "task-management",
    "ai-agents",
    "effect-ts",
    "mcp",
    "orchestration"
  ],
  "homepage": "https://txdocs.dev/",
  "author": "James Phoenix",
  "license": "MIT"
}
